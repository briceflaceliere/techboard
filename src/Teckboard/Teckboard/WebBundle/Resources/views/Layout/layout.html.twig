<!DOCTYPE html>
<html lang="fr" ng-app="teckboardApp">
    <head>
        <meta charset="UTF-8">
        <title>Teckboard</title>
        <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

        <!-- Bootstrap 3.3.2 -->
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <!-- FontAwesome 4.3.0 -->
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
        <!-- Ionicons 2.0.0 -->
        <link href="http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet" type="text/css" />

        {% stylesheets
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/css/AdminLTE.min.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/css/skins/skin-black.min.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/css/skins/_all-skins.min.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/iCheck/flat/blue.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/morris/morris.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/datepicker/datepicker3.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/daterangepicker/daterangepicker-bs3.css'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css'
        '@TeckboardWebBundle/Resources/public/libs/angular-loading-bar/build/loading-bar.min.css'
        '@TeckboardWebBundle/Resources/public/libs/spinkit/css/spinners/3-wave.css'
        filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" output='css/compiled/lte.css' />
        {% endstylesheets %}

        {% stylesheets
        '@TeckboardWebBundle/Resources/less/*'
        filter='less' %}
        <link rel="stylesheet" href="{{ asset_url }}" output='css/compiled/app.css' />
        {% endstylesheets %}

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="skin-blue fixed">
        <div id="loader-wrapper" data-loading>
            <div id="loader-area">
                <div class="sk-spinner sk-spinner-wave">
                    <div class="sk-rect1"></div>
                    <div class="sk-rect2"></div>
                    <div class="sk-rect3"></div>
                    <div class="sk-rect4"></div>
                    <div class="sk-rect5"></div>
                </div>
            </div>
        </div>
        <div class="wrapper">
            {% include "TeckboardWebBundle::Parcial/header.html.twig" %}

            {% include "TeckboardWebBundle::Parcial/sidebar.html.twig" %}

            <div class="content-wrapper">
                {% block content %}{% endblock %}
            </div>

            {% include "TeckboardWebBundle::Parcial/footer.html.twig" %}
        </div>


        <!-- ./wrapper -->
        <!-- jQuery 2.1.3 -->
        <script src="http://code.jquery.com/jquery-2.1.3.min.js"></script>
        <!-- jQuery UI 1.11.2 -->
        <script src="http://code.jquery.com/ui/1.11.2/jquery-ui.min.js" type="text/javascript"></script>
        <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>

        <!-- Bootstrap 3.3.2 JS -->
        <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js" type="text/javascript"></script>
        <!-- Morris.js charts -->
        <script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>

        {% javascripts
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/morris/morris.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/sparkline/jquery.sparkline.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/knob/jquery.knob.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/daterangepicker/daterangepicker.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/datepicker/bootstrap-datepicker.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/iCheck/icheck.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/plugins/fastclick/fastclick.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/js/app.min.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/js/pages/dashboard.js'
        '@TeckboardWebBundle/Resources/public/libs/AdminLTE/js/demo.js' output='js/compiled/lte.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts
        '@TeckboardWebBundle/Resources/public/libs/angular/angular.min.js'
        '@TeckboardWebBundle/Resources/public/libs/lodash/dist/lodash.min.js'
        '@TeckboardWebBundle/Resources/public/libs/restangular/dist/restangular.min.js'
        '@TeckboardWebBundle/Resources/public/libs/angular-loading-bar/build/loading-bar.min.js'
        '@TeckboardWebBundle/Resources/public/app/*.js'
        '@TeckboardWebBundle/Resources/public/app/configs/*.js'
        '@TeckboardWebBundle/Resources/public/app/services/*.js'
        '@TeckboardWebBundle/Resources/public/app/directives/*.js'
        '@TeckboardWebBundle/Resources/public/app/controllers/*.js'
         output='js/compiled/teckboard.js'%}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

    </body>
</html>